{
  "save_dir": "dcfn-green",
  "model": {
    "name": "dcfn",
    "params": {"d": [32, 64, 128]}
  },
  "scale": 16,
  "train_set": "green_train",
  "val_set": "green_valid",
  "test_sets": ["green_valid", "green_train"],
  "lr_sub_size": 10,
  "lr_sub_stride": 6,
  "lr_block_size": [30, 30],
  "epochs": 50,
  "optimizer": {
    "name": "adam",
    "params": {
      "lr": 1e-3,
      "decay": 1e-5
    }
  }
}
